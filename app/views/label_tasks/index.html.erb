<p id="notice"><%= notice %></p>

<h1>Label Tasks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @label_tasks.each do |label_task| %>
      <tr>
        <td><%= link_to 'Show', label_task %></td>
        <td><%= link_to 'Edit', edit_label_task_path(label_task) %></td>
        <td><%= link_to 'Destroy', label_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Label Task', new_label_task_path %>
